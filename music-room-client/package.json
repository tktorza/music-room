{
  "name": "music-room-client",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "fix": "yarn && rm -fr node_modules/nachos-ui/node_modules/react-native",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js",
    "lint": "eslint --fix src"
  },
  "eslintConfig": {
    "extends": [
      "zavatta"
    ],
    "plugins": [
      "react",
      "react-native"
    ],
    "rules": {
      "no-sync": 0,
      "no-undef": 0,
      "id-length": 0,
      "camelcase": 0,
      "no-inline-comments": 0,
      "max-nested-callbacks": 0,
      "max-statements-per-line": 0,
      "no-unused-vars": 0,
      "no-class-assign": 0
    }
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true,
  "dependencies": {
    "babel-preset-expo": "^4.0.0",
    "bluebird": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-zavatta": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "expo": "^25.0.0",
    "immutable": "^3.8.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "nachos-ui": "^0.1.2",
    "react": "16.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-25.0.0.tar.gz",
    "react-native-elements": "^0.19.0",
    "react-native-router-flux": "^4.0.0-beta.28",
    "react-native-ui-lib": "^3.3.35",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.2"
  }
}
